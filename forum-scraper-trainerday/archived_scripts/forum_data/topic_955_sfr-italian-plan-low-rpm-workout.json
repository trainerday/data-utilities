      "name": "",
      "username": "Alex",
      "avatar_template": "/user_avatar/forums.trainerday.com/alex/{size}/1331_2.png",
      "created_at": "2021-11-04T20:57:35.617Z",
      "cooked": "<p>In talking with one user that has a Tacx Flow we realized that some trainers that have a maximum power of 600 watts or so many not have enough resistance in their trainer to handle the loads of low RPM.  600 watts at 120 rpm is a maximum of 165 watts at 33 rpm and this is just approximate maximums so it might be less than this.</p>",
      "post_number": 8,
      "post_type": 1,
      "updated_at": "2021-11-04T20:57:35.617Z",
      "reply_count": 1,
      "reply_to_post_number": null,
      "quote_count": 0,
      "incoming_link_count": 1,
      "reads": 101,
      "readers_count": 100,
      "score": 30.2,
      "yours": false,
      "topic_id": 955,
